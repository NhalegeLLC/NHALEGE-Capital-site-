# Nhalege Capital Production Environment Variables
# Domain: capital.nhalege.com

# Backend URL for production
REACT_APP_BACKEND_URL=https://capital.nhalege.com

# Google Analytics 4 Configuration
REACT_APP_GA4_MEASUREMENT_ID=G-XXXXXXXXXX

# Google Tag Manager Configuration  
REACT_APP_GTM_ID=GTM-XXXXXXX

# Mailchimp Integration
REACT_APP_MAILCHIMP_API_KEY=your_mailchimp_api_key_here
REACT_APP_MAILCHIMP_SERVER=us1
REACT_APP_MAILCHIMP_AUDIENCE_ID=your_audience_id_here

# Site Configuration
REACT_APP_SITE_NAME=Nhalege Capital
REACT_APP_SITE_URL=https://capital.nhalege.com
REACT_APP_SUPPORT_EMAIL=support@nhalege.com

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_MAILCHIMP=true
REACT_APP_ENABLE_RETARGETING=true

# Deployment Configuration
GENERATE_SOURCEMAP=false
REACT_APP_VERSION=1.0.0